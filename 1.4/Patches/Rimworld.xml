                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- SteamGeyser -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="SteamGeyser"]/building/relatedBuildCommands</xpath>
                                <value>
