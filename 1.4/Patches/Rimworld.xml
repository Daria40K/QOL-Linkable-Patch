<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <!-- ========== RimWorld ==========

        BasicBedBase | #bed
        HospitalBed | #medical

        == Generators
		WoodFiredGenerator | #generator
        ChemfuelPoweredGenerator | #generator #chemfuel
        WindTurbine | #generator
        SolarGenerator | #generator
        GeothermalGenerator | #generator #geothermal
        WatermillGenerator | #generator
        ToxifierGenerator | #generator

        == Cables
        PowerConduit | #cable
        WaterproofConduit | #cable
	-->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Core</li>
        </mods>

        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <!-- BasicBedBase | HospitalBed -->
                <li Class="PatchOperationConditional">
                    <success>Always</success>
                    <xpath>Defs/ThingDef[@Name="BasicBedBase" or defName="HospitalBed"]/building</xpath>
                    <match Class="PatchOperationConditional">
                        <success>Always</success>
                        <xpath>Defs/ThingDef[@Name="BasicBedBase" or defName="HospitalBed"]/building/relatedBuildCommands</xpath>
                        <nomatch Class="PatchOperationAdd">
                            <success>Always</success>
                            <xpath>Defs/ThingDef[@Name="BasicBedBase" or defName="HospitalBed"]/building</xpath>
                            <value>
                                <relatedBuildCommands>

                                </relatedBuildCommands>
                            </value>
                        </nomatch>
                    </match>
                    <nomatch Class="PatchOperationAdd">
                        <success>Always</success>
                        <xpath>Defs/ThingDef[@Name="BasicBedBase" or defName="HospitalBed"]</xpath>
                        <value>
                            <building>
                                <relatedBuildCommands>

                                </relatedBuildCommands>
                            </building>
                        </value>
                    </nomatch>
                </li>

                <!-- WoodFiredGenerator | ChemfuelPoweredGenerator | WindTurbine | SolarGenerator | GeothermalGenerator | WatermillGenerator | ToxifierGenerator -->
                <li Class="PatchOperationConditional">
                    <success>Always</success>
                    <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building</xpath>
                    <match Class="PatchOperationConditional">
                        <success>Always</success>
                        <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                        <nomatch Class="PatchOperationAdd">
                            <success>Always</success>
                            <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building</xpath>
                            <value>
                                <relatedBuildCommands>

                                </relatedBuildCommands>
                            </value>
                        </nomatch>
                    </match>
                    <nomatch Class="PatchOperationAdd">
                        <success>Always</success>
                        <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]</xpath>
                        <value>
                            <building>
                                <relatedBuildCommands>

                                </relatedBuildCommands>
                            </building>
                        </value>
                    </nomatch>
                </li>

                <!-- ToxifierGenerator -->
                <li Class="PatchOperationConditional">
                    <success>Always</success>
                    <xpath>Defs/ThingDef[defName="ToxifierGenerator"]/building</xpath>
                    <match Class="PatchOperationConditional">
                        <success>Always</success>
                        <xpath>Defs/ThingDef[defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                        <nomatch Class="PatchOperationAdd">
                            <success>Always</success>
                            <xpath>Defs/ThingDef[defName="ToxifierGenerator"]/building</xpath>
                            <value>
                                <relatedBuildCommands>

                                </relatedBuildCommands>
                            </value>
                        </nomatch>
                    </match>
                    <nomatch Class="PatchOperationAdd">
                        <success>Always</success>
                        <xpath>Defs/ThingDef[defName="ToxifierGenerator"]</xpath>
                        <value>
                            <building>
                                <relatedBuildCommands>

                                </relatedBuildCommands>
                            </building>
                        </value>
                    </nomatch>
                </li>

                <!-- Door | Autodoor -->
                <li Class="PatchOperationConditional">
                    <success>Always</success>
                    <xpath>Defs/ThingDef[defName="Door" or defName="Autodoor"]/building</xpath>
                    <match Class="PatchOperationConditional">
                        <success>Always</success>
                        <xpath>Defs/ThingDef[defName="Door" or defName="Autodoor"]/building/relatedBuildCommands</xpath>
                        <nomatch Class="PatchOperationAdd">
                            <success>Always</success>
                            <xpath>Defs/ThingDef[defName="Door" or defName="Autodoor"]/building</xpath>
                            <value>
                                <relatedBuildCommands>

                                </relatedBuildCommands>
                            </value>
                        </nomatch>
                    </match>
                    <nomatch Class="PatchOperationAdd">
                        <success>Always</success>
                        <xpath>Defs/ThingDef[defName="Door" or defName="Autodoor"]</xpath>
                        <value>
                            <building>
                                <relatedBuildCommands>

                                </relatedBuildCommands>
                            </building>
                        </value>
                    </nomatch>
                </li>

                <!-- ========== RimWorld ========== -->
                <!-- WoodFiredGenerator | ChemfuelPoweredGenerator | WindTurbine | SolarGenerator | GeothermalGenerator | WatermillGenerator | ToxifierGenerator -->
                <li Class="PatchOperationAdd">
                    <success>Always</success>
                    <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                    <value>
                        <li>PowerConduit</li>
                        <li>WaterproofConduit</li>
                    </value>
                </li>

                <!-- ========== Advanced Power Plus ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Advanced Power Plus</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- SteamGeyser -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="SteamGeyser"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>sd_adv_powergen_adv_geothermal_generator</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== Akeron - Decorations ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Akeron - Decorations</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- WoodFiredGenerator | ChemfuelPoweredGenerator | WindTurbine | SolarGenerator | GeothermalGenerator | WatermillGenerator | ToxifierGenerator -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>AKN_NeatCables</li>
                                    <li>AKN_DecorativeCables</li>
                                    <li>AKN_OverheadCables</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== Bill Doors' reinforced wires ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Bill Doors' reinforced wires</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- WoodFiredGenerator | ChemfuelPoweredGenerator | WindTurbine | SolarGenerator | GeothermalGenerator | WatermillGenerator | ToxifierGenerator -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>BD_ReinforcedPowerConduit</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== Cybernetic Storm ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Cybernetic Storm</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- WoodFiredGenerator | ChemfuelPoweredGenerator | WindTurbine | SolarGenerator | GeothermalGenerator | WatermillGenerator | ToxifierGenerator -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>CS_PowerConduit</li>
                                    <li>WaterproofUltrasteelConduit</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== Door Mat ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Door Mat</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- Door | Autodoor -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="Door" or defName="Autodoor"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>LT_DoorMatLeather</li>
                                    <li>LT_DoorMatBigLeather</li>
                                    <li>LT_DoorMatRed</li>
                                    <li>LT_DoorMatOrange</li>
                                    <li>LT_DoorMatGreen</li>
                                    <li>LT_DoorMatTeal</li>
                                    <li>LT_DoorMatBlue</li>
                                    <li>LT_DoorMatBigRed</li>
                                    <li>LT_DoorMatBigOrange</li>
                                    <li>LT_DoorMatBigGreen</li>
                                    <li>LT_DoorMatBigTeal</li>
                                    <li>LT_DoorMatBigBlue</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== Glitter Tech ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Glitter Tech</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- WoodFiredGenerator | ChemfuelPoweredGenerator | WindTurbine | SolarGenerator | GeothermalGenerator | WatermillGenerator | ToxifierGenerator -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>HeavyPowerConduit</li>
                                </value>
                            </li>

                            <!-- SteamGeyser -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="SteamGeyser"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>XenonIonTurbine</li>
                                </value>
                            </li>

                            <!-- Wall -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="Wall"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>BlastDoor</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== M-13's Safe Batteries and Conduits ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>M-13's Safe Batteries and Conduits</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- WoodFiredGenerator | ChemfuelPoweredGenerator | WindTurbine | SolarGenerator | GeothermalGenerator | WatermillGenerator | ToxifierGenerator -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>M13PowerConduit</li>
                                    <li>M13WaterproofConduit</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== UN-Colony Vanilla (Continued) ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>UN-Colony Vanilla (Continued)</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- WoodFiredGenerator | ChemfuelPoweredGenerator | WindTurbine | SolarGenerator | GeothermalGenerator | WatermillGenerator | ToxifierGenerator -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>SadjuukPowerConduitA</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== Backup Power ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Backup Power</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- WoodFiredGenerator | ChemfuelPoweredGenerator -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>BackupPower_Attachment</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== GlitterNet ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>GlitterNet</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- BasicBedBase -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[@Name="BasicBedBase"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>GNBasicBedIntegrator</li>
                                    <li>GNImprovedBedIntegrator</li>
                                    <li>GNAdvancedBedIntegrator</li>
                                </value>
                            </li>

                            <!-- HospitalBed -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="HospitalBed"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>GNBasicMedicalIntegrator</li>
                                    <li>GNImprovedMedicalIntegrator</li>
                                    <li>GNAdvancedMedicalIntegrator</li>
                                </value>
                            </li>

                            <!-- SimpleResearchBench | HiTechResearchBench -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="SimpleResearchBench" or defName="HiTechResearchBench"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
                                <value>
                                    <li>GNBasicResearchIntegrator</li>
                                    <li>GNImprovedResearchIntegrator</li>
                                    <li>GNAdvancedResearchIntegrator</li>
                                </value>
                            </li>

                            <!-- WoodFiredGenerator | ChemfuelPoweredGenerator | WindTurbine | SolarGenerator | GeothermalGenerator | WatermillGenerator | ToxifierGenerator -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="WoodFiredGenerator" or defName="ChemfuelPoweredGenerator" or defName="WindTurbine" or defName="SolarGenerator" or defName="GeothermalGenerator" or defName="WatermillGenerator" or defName="ToxifierGenerator"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>GNBasicPowerIntegrator</li>
                                    <li>GNImprovedPowerIntegrator</li>
                                    <li>GNAdvancedPowerIntegrator</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== M-13's Geothermal Generators ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>M-13's Geothermal Generators</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- SteamGeyser -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="SteamGeyser"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>Tweaked_Geothermal</li>
                                    <li>Tweaked_Geothermal_NG</li>
                                    <li>Upgraded_Geothermal</li>
                                    <li>Upgraded_Geothermal_NG</li>
                                    <li>Advanced_Geothermal</li>
                                    <li>Advanced_Geothermal_NG</li>
                                    <li>Ultimate_Geothermal</li>
                                    <li>Ultimate_Geothermal_NG</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== Obsidia Expansion ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Obsidia Expansion</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- SteamGeyser -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="SteamGeyser"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>OE_GeothermalGenerator</li>
                                </value>
                            </li>
                        </operations>
                    </match>
