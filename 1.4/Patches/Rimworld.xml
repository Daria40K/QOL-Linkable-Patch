                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>[WD] Reinforced Doors</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- Wall -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="Wall"]/building/relatedBuildCommands</xpath>
                                <value>
                                    <li>WDReinforcedDoor</li>
                                    <li>WDReinforcedAutodoor</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>

                <!-- ========== Vanilla Chemfuel Expanded ========== -->
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Vanilla Chemfuel Expanded</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <success>Always</success>
                        <operations>
                            <!-- ChemfuelPoweredGenerator -->
                            <li Class="PatchOperationAdd">
                                <success>Always</success>
                                <xpath>Defs/ThingDef[defName="ChemfuelPoweredGenerator"]/building/relatedBuildCommands</xpath>
